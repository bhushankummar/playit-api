{
    "name": "playit-api",
    "version": "1.0.0",
    "description": "Playit API for Web/Mobile/Desktop APP",
    "main": "dist",
    "scripts": {
        "start": "npm run version && export DEBUG=PL:* && nodemon -x 'ts-node src/server.ts;'",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "prebuild": "npm run lint && rm -rf ./dist/src/* && rm -rf ./dist/test/*",
        "build": "npm run build-ts",
        "build-ts": "tsc",
        "serve-build": "npm run version && node dist/src/server.js",
        "unit": "mocha -r ts-node/register --config test/.mocharc.json",
        "posttest": "npm run lint",
        "version": "echo 'youtube-dl' ; ./node_modules/youtube-dl/bin/youtube-dl --version"
    },
    "cacheDirectories": [
        "media",
        "app/dist/media",
        "dist/media"
    ],
    "engines": {
        "node": "10.16.3",
        "npm": "6.9.0"
    },
    "author": "Bhushankumar L (bhushankumar.lilapara@gmail.com)",
    "license": "ISC",
    "dependencies": {
        "@typeform/eslint-config": "^3.0.2",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "boom": "^7.3.0",
        "cors": "^2.8.5",
        "cron": "^1.8.2",
        "debug": "^4.3.1",
        "express": "^4.17.1",
        "ffmpeg-static": "^4.2.7",
        "find": "^0.3.0",
        "fs-extra": "^9.1.0",
        "googleapis": "^67.1.1",
        "googleapis-common": "^5.0.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "mongodb": "^3.6.5",
        "node-pre-gyp": "^0.14.0",
        "nodemon": "^2.0.7",
        "passport": "^0.4.1",
        "passport-http-bearer": "^1.0.1",
        "path": "^0.12.7",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "request": "^2.88.0",
        "request-promise": "^4.2.5",
        "typeorm": "^0.2.31",
        "uuid": "^8.3.2",
        "youtube-dl": "^3.0.2",
        "youtube-dl-exec": "^1.1.3"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.33",
        "@types/body-parser": "^1.19.0",
        "@types/boom": "^7.3.0",
        "@types/chai": "^4.2.15",
        "@types/chai-http": "^4.2.0",
        "@types/cors": "^2.8.10",
        "@types/cron": "^1.7.2",
        "@types/debug": "4.1.5",
        "@types/expect": "^24.3.0",
        "@types/express": "^4.17.11",
        "@types/find": "^0.2.1",
        "@types/fs-extra": "^9.0.8",
        "@types/lodash": "^4.14.168",
        "@types/mocha": "^8.2.1",
        "@types/mongodb": "^3.6.10",
        "@types/node": "^14.14.35",
        "@types/passport": "^1.0.6",
        "@types/passport-http-bearer": "^1.0.36",
        "@types/request-promise": "^4.1.47",
        "@types/supertest": "^2.0.10",
        "@types/uuid": "^8.3.0",
        "@types/youtube-dl": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.2",
