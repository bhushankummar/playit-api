{
    "diff": true,
    "extension": [
        "ts",
        "js",
        "cjs",
        "mjs"
    ],
    "package": "package.json",
    "reporter": "spec",
    "slow": "75",
    "timeout": "2000",
    "ui": "bdd",
    "spec": "src/**/*.spec.ts",
    "watch-files": [
        "src/**/*.spec.ts",
        "test/**/*.spec.ts"
    ],
    "watch-ignore": [
        "node_modules",
        "lib/vendor"
    ]
}